{
  "manifest_version": 3,
  "name": "YouTube Bulk URL Scraper",
  "version": "1.1",
  "description": "Scrapes video URLs from YouTube search results via auto-scroll.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/results*"],
      "js": ["content.js"]
    }
  ]
}